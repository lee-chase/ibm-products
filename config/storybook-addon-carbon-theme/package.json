{
  "name": "@carbon/storybook-addon-theme",
  "description": "Carbon theme switcher for Storybook",
  "version": "2.0.3",
  "license": "Apache-2.0",
  "main": "dist/react.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/ibm-products.git",
    "directory": "config/storybook-addon-carbon-theme"
  },
  "bugs": "https://github.com/carbon-design-system/ibm-products/issues",
  "homepage": "https://github.com/storybookjs/storybook#readme",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "keywords": [
    "addon",
    "carbon",
    "theme",
    "storybook",
    "carbon design system",
    "carbon community",
    "carbon for cloud & cognitive",
    "carbon for ibm products"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean build:js",
    "build:js": "babel src --out-dir dist -s",
    "prepare": "npm run build",
    "//upgrade-dependencies": "# don't upgrade carbon (done globally)",
    "upgrade-dependencies": "npm-check-updates -u --dep dev,peer,prod --color --reject '/(carbon)/'"
  },
  "peerDependencies": {
    "react": "*",
    "vue": "*"
  },
  "dependencies": {
    "@storybook/addons": "^7.4.0",
    "@storybook/api": "^7.4.0",
    "@storybook/client-api": "^7.4.0",
    "@storybook/client-logger": "^7.4.0",
    "@storybook/components": "^7.4.0",
    "@storybook/core-events": "^7.4.0",
    "@storybook/theming": "^7.4.0",
    "core-js": "^3.31.1",
    "global": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "npm-check-updates": "^16.10.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1"
  }
}
